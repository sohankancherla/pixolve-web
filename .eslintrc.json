{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "operator-linebreak": "off",
    "react/jsx-props-no-spreading": "off",
    "react/require-default-props": "off"
  },
  "ignorePatterns": ["node_modules/", ".next/", "out/", "ui", "route.ts"]
}
